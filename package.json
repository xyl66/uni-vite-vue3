    "husky": "^8.0.3",
    "jest": "27.0.4",
    "jest-environment-node": "27.5.1",
    "lint-staged": "^13.2.1",
    "node-simctl": "^7.1.12",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "puppeteer": "14.0.0",
